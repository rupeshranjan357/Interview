#include <iostream>
#include <vector>
using namespace std;
//array deletion
int arrdel(int myArray[],int n,int x){
    int i;
    for(int i=0;i<n;i++){
        if(myArray[i]==x){
            break;
        }
    }
        if(i==n) return n;
        for(int j=i;j<n-1;j++){
            myArray[j]=myArray[j+1];
        }
        return n-1;
    }
//is sroted array
bool is_sorted(int arr[],int n){
    for(int i=0;i<n;i++){
        if(arr[i]>arr[i+1]){
            return false;
        }
    }
    return true;
}
// reverse the array
void reverse_arr(int arr[],int n){
    int l=0,r=n-1;
    while(l<r){
        int temp=arr[l];
        arr[l]=arr[r];
        arr[r]=temp;
        l++;r--;
    }
     for (int i=0;i<n;i++) {
        cout << arr[i]<< " ";
    }
    return;
}

int main() {
    // Generate an array of integers from 1 to 10
    // vector<int> myArray;
    int myArray[5];
    int a;
    for (int i = 0; i < 5; i++) {
    //   cin>>a;
    //   myArray.push_back(a);
    cin>>myArray[i];
    }
    int x;
    cout<<"Enter the array u want to delte";
    cin>>x;
    int n=sizeof(myArray)/(sizeof(myArray[0]));
    int ans=arrdel( myArray,n,x);
    cout<<"Delted Array"<<ans<<endl;
    for (int num : myArray) {
        cout << num << " ";
    }
    return 0;
}
